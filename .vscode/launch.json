{
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Debug TimesFM (Zero Shot)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/run.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": false,
            "env": {
                "HF_ENDPOINT": "https://hf-mirror.com"
            },
            "args": [
                "--task_name", "zero_shot_forecast",
                "--is_training", "0",
                "--root_path", "./dataset/ETT-small/",
                "--data_path", "ETTh1.csv",
                "--model_id", "ETTh1_512_96",
                "--model", "TimesFM",
                "--data", "ETTh1",
                "--features", "M",
                "--seq_len", "512",
                "--pred_len", "96",
                "--enc_in", "7",
                "--des", "Exp",
                "--itr", "1",
                "--use_gpu", "True",
                
                // 关键参数：防止内存爆炸
                "--num_workers", "0",
                // 关键参数：控制显存和编译速度
                "--batch_size", "16" 
            ]
        }
    ]
}